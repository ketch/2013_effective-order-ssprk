keywords = {epi schemes,exponential propagation iterative methods,exponential time differencing schemes,fast time integrators,krylov projections,numerical methods,stiff systems},
month = apr,
number = {2},
pages = {748--776},
title = {Efficient integration of large stiff systems of ODEs with exponential propagation iterative (EPI) methods},
url = {http://dx.doi.org/10.1016/j.jcp.2005.08.032},
volume = {213},
year = {2006}
}
